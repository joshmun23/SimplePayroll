<h1>Listing select_employees</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @select_employees.each do |select_employee| %>
      <tr>
        <td><%= link_to 'Show', select_employee %></td>
        <td><%= link_to 'Edit', edit_select_employee_path(select_employee) %></td>
        <td><%= link_to 'Destroy', select_employee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Select employee', select_path %>
